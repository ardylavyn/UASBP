
package FinalProjectUAS;
import java.io.*;
public class Login {
 //deklarasi
 private String username, password, nama;
 
 //constructor
 public Login()
 {
 password = "1234";
 nama = "Admin";
 }
 
 public Login(String password, String nama)
 {
   this.password = password;
   this.nama = nama;
 }
 
 //setter

 public void setPassword(String password)
 {
 this.password = password;
 } 
 
 public void setNama(String nama)
 {
 this.nama = nama;
 } 
 
 //getter
 public String getPassword()
 {
 return password;
 }
  public String getNama()
 {
 return nama;
 }

}

